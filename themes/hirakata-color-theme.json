{
  "name": "Better Solarized Dark",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#788192"
      }
    },
    {
      "name": "Liquid/Nunjucks/Jekyll Tags",
      "scope": [
        "punctuation.output.jekyll",
        "punctuation.tag.jekyll",
        "punctuation.frontmatter.jekyll",
        "punctuation.output.liquid",
        "punctuation.tag.liquid",
        "punctuation.output.njk",
        "punctuation.tag.njk"
      ],
      "settings": {
        "foreground": "#D1D6DF"
      }
    },
    {
      "name": "Jekyll Operators",
      "scope": [
        "entity.name.tag.jekyll",
        "entity.name.tag.liquid",
        "entity.name.tag.njk"
      ],
      "settings": {
        "foreground": "#A3759B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Jekyll Variable",
      "scope": [
        "punctuation.tag.jekyll",
        "punctuation.tag.liquid",
        "punctuation.tag.njk"
      ],
      "settings": {
        "foreground": "#C5CAD6"
      }
    },
    {
      "name": "Jekyll Content",
      "scope": [
        "variable.other.jekyll",
        "variable.other.liquid",
        "variable.other.njk"
      ],
      "settings": {
        "foreground": "#C5CAD6"
      }
    },
    {
      "name": "Jekyll arguments",
      "scope": [
        "support.function.with-args.jekyll"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Javascript JSDoc Symbol",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#D1D6DF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#D57076"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#D57076"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8893D1"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
